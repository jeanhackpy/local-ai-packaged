{
  "models": {
    "providers": {
      "openrouter": {
        "baseUrl": "https://openrouter.ai/api/v1",
        "api": "openai-completions",
        "rateLimit": {
          "requestsPerDay": 1000,
          "minDelayMs": 4000
        },
        "models": [
          {
            "id": "openrouter/openrouter/free",
            "name": "OpenRouter Free (1000/day - Rate Limited)",
            "reasoning": false,
            "input": ["text"],
            "cost": { "input": 0, "output": 0, "cacheRead": 0, "cacheWrite": 0 },
            "contextWindow": 200000,
            "maxTokens": 8192
          }
        ]
      },
      "ollama": {
        "baseUrl": "http://ollama:11434/v1",
        "api": "openai-completions",
        "models": [
          {
            "id": "qwen2.5:7b-instruct-q4_K_M",
            "name": "Qwen 2.5 7B Instruct (Local - CPU Optimized)",
            "reasoning": false,
            "input": ["text"],
            "cost": { "input": 0, "output": 0, "cacheRead": 0, "cacheWrite": 0 },
            "contextWindow": 8192,
            "maxTokens": 4096
          },
          {
            "id": "nomic-embed-text",
            "name": "Nomic Embed Text (Local Embeddings)",
            "reasoning": false,
            "input": ["text"],
            "cost": { "input": 0, "output": 0, "cacheRead": 0, "cacheWrite": 0 },
            "contextWindow": 2048,
            "maxTokens": 512
          }
        ]
      }
    }
  },
  "routing": {
    "strategy": "cost-optimized",
    "fallbacks": [
      "openrouter/openrouter/free",
      "ollama/qwen2.5:7b-instruct-q4_K_M"
    ]
  },
  "integrations": {
    "databases": {
      "supabase": {
        "enabled": true,
        "url": "${SUPABASE_URL}",
        "serviceKey": "${SUPABASE_KEY}",
        "anonKey": "${SUPABASE_ANON_KEY}",
        "description": "PostgreSQL database with real-time subscriptions and auth"
      },
      "qdrant": {
        "enabled": true,
        "url": "${QDRANT_URL}",
        "description": "Vector database for embeddings and similarity search"
      },
      "neo4j": {
        "enabled": true,
        "url": "${NEO4J_URL}",
        "auth": "${NEO4J_AUTH}",
        "description": "Graph database for relationship mapping"
      },
      "postgres": {
        "enabled": true,
        "host": "${POSTGRES_HOST}",
        "port": "${POSTGRES_PORT}",
        "user": "${POSTGRES_USER}",
        "password": "${POSTGRES_PASSWORD}",
        "database": "${POSTGRES_DB}",
        "description": "Direct PostgreSQL connection"
      }
    },
    "services": {
      "n8n": {
        "enabled": true,
        "url": "${N8N_URL}",
        "webhookUrl": "${N8N_WEBHOOK_URL}",
        "description": "Workflow automation and integrations"
      },
      "crawl4ai": {
        "enabled": true,
        "url": "${CRAWL4AI_URL}",
        "description": "Web crawling and data extraction"
      }
    }
  }
}
